# Development Environment Configuration
# Copy this file to .env when using docker-compose.dev.yml

# Application Settings
APP_NAME="Mainframe Studio (Dev)"
DEBUG_MODE=True
SECRET_KEY="dev-secret-key-change-in-production"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=30999
ASSISTANT_AGENT_MODEL="gpt-4o"

AZURE_OPENAI_API_BASE="https://your-azure-openai-endpoint.openai.azure.com/"
AZURE_OPENAI_API_KEY="your-azure-openai-api-key"
AZURE_OPENAI_MODEL="gpt-4.1"
AZURE_OPENAI_MODEL_TYPE="chat"
AZURE_OPENAI_API_VERSION="2024-10-21"
 
# Azure Storage Settings
AZURE_STORAGE_CONNECTION_STRING="your-azure-storage-connection-string"
AZURE_STORAGE_CONTAINER_NAME="repositories"
AZURE_STORAGE_URL="https://your-storage-account.blob.core.windows.net/"
 
# Git Settings
GIT_ASKPASS=false
GIT_TERMINAL_PROMPT=0
 
# LLM Settings
MODEL_NAME="gpt-4o"
LLM_PROVIDER="azure_openai"
 
# Nebula Database Settings
NEBULA_HOST="localhost"
NEBULA_PORT="9669"
NEBULA_USERNAME=""
NEBULA_PASSWORD=""
NEBULA_MAX_CONNECTION_POOL_SIZE=10
 
# Database Settings (Development - Docker Compose)
MONGO_HOST="mongodb"
MONGO_PORT=27017
MONGO_USER="mainframe_user"
MONGO_PASSWORD="dev_password"
MONGO_DATABASE="mainframe"
REDIS_URL="redis://redis:6379/0"

# MongoDB Environment Variables (for docker-compose)
MONGO_INITDB_ROOT_USERNAME="admin"
MONGO_INITDB_ROOT_PASSWORD="123qwe"
MONGO_INITDB_DATABASE="mainframe"
 
MILVUS_HOST=127.0.0.1
MILVUS_PORT=19530
 
TEI_URL=""
CHUNK_SIZE=1000
CHUNK_OVERLAP=100
 
# Assistant Agent Settings
ASSISTANT_AGENT_MODEL="gpt-4"
