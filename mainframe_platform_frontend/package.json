{
  "name": "base-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "set DISABLE_ESLINT_PLUGIN=true && craco start",
    "start:pro": "craco start",
    "start:dev": "DISABLE_ESLINT_PLUGIN=true craco start",
    "check:type": "tsc --project tsconfig.json --noEmit",
    "check:lint": "eslint . --fix  --ext .js,.jsx,.ts,.tsx --quiet",
    "check:prettier": "prettier --write --ignore-unknown \"**/*\" ",
    "check:all": "yarn check:prettier && yarn check:lint",
    "deploy:pro": "yarn build && az login && az storage blob upload-batch --account-name cobolweb -s ./build -d '$web' --overwrite",
    "deploy:dev": "yarn build && az login && az storage blob upload-batch --account-name cobolweb -s ./build -d '$web' --overwrite",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.4",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.1.1",
    "@llm-ui/code": "^0.13.3",
    "@llm-ui/markdown": "^0.13.3",
    "@llm-ui/react": "^0.13.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@tanstack/react-query": "^5.63.0",
    "@types/event-source-polyfill": "^1.0.5",
