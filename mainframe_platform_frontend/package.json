    "chart.js": "^4.4.4",
    "d3": "^7.8.5",
    "event-source-polyfill": "^1.0.31",
    "html-react-parser": "^5.1.16",
    "i18next": "^23.2.6",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "query-string": "^9.1.1",
    "re-resizable": "^6.9.11",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-content-loader": "^7.0.2",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^5.0.0",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^13.0.1",
    "react-markdown": "^9.0.1",
